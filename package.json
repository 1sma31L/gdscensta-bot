{
    "name": "telegram-bot",
    "type": "module",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "dev": "concurrently \"npm run clean && tsc -w\" \"nodemon dist/index.js\"",
        "lint": "eslint ./src/**/*.ts --fix",
        "format": "prettier --write ./**/*.{ts,json}",
        "clean": "rm -rf dist/"
    },
    "pre-commit": [
        "build",
        "lint",
        "format"
    ],
    "keywords": [],
    "author": "BOUSSEKINE M. Ismail",
    "license": "MIT",
    "description": "GDSC ENSTA Registartions Bot.",
    "dependencies": {
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "morgan": "^1.10.0",
        "node-telegram-bot-api": "^0.66.0",
        "telegraf": "^4.16.3"
    },
    "devDependencies": {
        "@types/dotenv": "^8.2.3",
        "@types/express": "^5.0.0",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.9.0",
        "@types/node-telegram-bot-api": "^0.64.7",
        "concurrently": "^9.1.0",
        "eslint": "^9.15.0",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3"
    }
}
